#!/bin/sh

LC_ALL=C

function ceildiv() {
  local num=$1
  local div=$2
  echo $(( (num + div - 1) / div ))
}
function get_cpuset() {
  local cpusetroot=$1
  local cpusetfile=$2
  local ncpu=0
  local count=
  [ -f "$cpusetroot/$cpusetfile" ] || return 1
  for token in $( tr ',' ' ' < "$cpusetroot/$cpusetfile" ); do
    case "$token" in
      *-*)
        count=$( seq $(echo "$token" | tr '-' ' ') | wc -l )
        ncpu=$(( ncpu+count ))
        ;;
      *)
        ncpu=$(( ncpu+1 ))
        ;;
    esac
  done
  echo "$ncpu"
}
function get_quota() {
  local cpuroot=$1
  local ncpu=0
  [ -f "$cpuroot/cpu.cfs_quota_us" ] || return 1
  [ -f "$cpuroot/cpu.cfs_period_us" ] || return 1
  local cfs_quota=$( cat "$cpuroot/cpu.cfs_quota_us" )
  local cfs_period=$( cat "$cpuroot/cpu.cfs_period_us" )
  [ "$cfs_quota" = "-1" ] && return 1
  [ "$cfs_period" = "0" ] && return 1
  ncpu=$( ceildiv "$cfs_quota" "$cfs_period" )
  [ "$ncpu" -gt 0 ] || return 1
  echo "$ncpu"
}
function get_quota_v2() {
  local cpuroot=$1
  local ncpu=0
  [ -f "$cpuroot/cpu.max" ] || return 1
  local cfs_quota=$( cut -d' ' -f 1 < "$cpuroot/cpu.max" )
  local cfs_period=$( cut -d' ' -f 2 < "$cpuroot/cpu.max" )
  [ "$cfs_quota" = "max" ] && return 1
  [ "$cfs_period" = "0" ] && return 1
  local ncpu=$( ceildiv "$cfs_quota" "$cfs_period" )
  [ "$ncpu" -gt 0 ] || return 1
  echo "$ncpu"
}
function get_cgroup_v1_path() {
  local needle=$1
  local found=
  local foundroot=
  local mountpoint=
  local controller=
  [ -r "/proc/self/mountinfo" ] || return 1
  [ -r "/proc/self/cgroup" ] || return 1
  while IFS= read -r line; do
    case "$needle" in
      "cpuset")
        case "$line" in
          *cpuset*)
            found=$( echo "$line" | cut -d ' ' -f 4,5 )
            break
            ;;
        esac
        ;;
      "cpu")
        case "$line" in
          *cpuset*)
            ;;
          *cpu,cpuacct*|*cpuacct,cpu|*cpuacct*|*cpu*)
            found=$( echo "$line" | cut -d ' ' -f 4,5 )
            break
            ;;
        esac
    esac
  done << __EOF__
$( grep -F -- '- cgroup ' /proc/self/mountinfo )
__EOF__
  while IFS= read -r line; do
    controller=$( echo "$line" | cut -d: -f 2 )
    case "$needle" in
      "cpuset")
        case "$controller" in
          cpuset)
            mountpoint=$( echo "$line" | cut -d: -f 3 )
            break
            ;;
        esac
        ;;
      "cpu")
        case "$controller" in
          cpu,cpuacct|cpuacct,cpu|cpuacct|cpu)
            mountpoint=$( echo "$line" | cut -d: -f 3 )
            break
            ;;
        esac
        ;;
    esac
done << __EOF__
$( grep -F -- 'cpu' /proc/self/cgroup )
__EOF__

  case "${found%% *}" in
    "/")
      foundroot="${found##* }$mountpoint"
      ;;
    "$mountpoint")
      foundroot="${found##* }"
      ;;
  esac
  echo "$foundroot"
}
function get_cgroup_v2_path() {
  found=
  foundroot=
  mountpoint=

  [ -r "/proc/self/mountinfo" ] || return 1
  [ -r "/proc/self/cgroup" ] || return 1

  while IFS= read -r line; do
    found=$( echo "$line" | cut -d ' ' -f 4,5 )
  done << __EOF__
$( grep -F -- '- cgroup2 ' /proc/self/mountinfo )
__EOF__

  while IFS= read -r line; do
    mountpoint=$( echo "$line" | cut -d: -f 3 )
done << __EOF__
$( grep -F -- '0::' /proc/self/cgroup )
__EOF__

  case "${found%% *}" in
    "")
      return 1
      ;;
    "/")
      foundroot="${found##* }$mountpoint"
      ;;
    "$mountpoint")
      foundroot="${found##* }"
      ;;
  esac
  echo "$foundroot"
}
function install_nginx() {
    echo "Installing nginx..."
    cd /opt
    apk -q add --update ca-certificates openssl tzdata nginx
    mkdir -p /var/lib/nginx /usr/share/nginx
    check_gid=$(grep :101: /etc/group | cut -d':' -f1)
    [ -z "${check_gid}" ] || groupmod -g 599 ${check_gid}
    addgroup -g 101 -S nginx || groupmod -g 101 nginx
    adduser -S -D -H -u 101 -h /var/cache/nginx -s /sbin/nologin -G nginx -g nginx nginx || usermod -u 101 -d /var/cache/nginx -s /sbin/nologin -g nginx nginx
    chown -R nginx:nginx /var/lib/nginx /usr/share/nginx
    local nginxPackages="nginx=${NGINX_VERSION}-r1 \
        nginx-module-xslt=${NGINX_VERSION}-r1 \
        nginx-module-geoip=${NGINX_VERSION}-r1 \
        nginx-module-image-filter=${NGINX_VERSION}-r1 \
        nginx-module-njs=${NGINX_VERSION}.${NJS_VERSION}-r1"
    local key_sha512="${NGINX_SIGNING_CHECKSUM} *stdin"
    wget -O /tmp/nginx_signing.rsa.pub https://nginx.org/keys/nginx_signing.rsa.pub
    if [ "$(openssl rsa -pubin -in /tmp/nginx_signing.rsa.pub -text -noout | openssl sha512 -r)" = "$key_sha512" ]; then
        echo "key verification succeeded!"
        mv /tmp/nginx_signing.rsa.pub /etc/apk/keys/
    else
        echo "key verification failed!"
        exit 1
    fi
    apk add -X "https://nginx.org/packages/alpine/v$(egrep -o '^[0-9]+\.[0-9]+' /etc/alpine-release)/main" --no-cache $nginxPackages
    if [ -n "/etc/apk/keys/nginx_signing.rsa.pub" ]; then
        rm -f /etc/apk/keys/nginx_signing.rsa.pub
    fi
    apk add --no-cache --virtual .gettext gettext
    mv /usr/bin/envsubst /tmp/
    local runDeps="$( \
        scanelf --needed --nobanner /tmp/envsubst \
            | awk '{ gsub(/,/, "\nso:", $2); print "so:" $2 }' \
            | sort -u \
            | xargs -r apk info --installed \
            | sort -u \
    )"
    apk add --no-cache $runDeps
    apk del .gettext
    mv /tmp/envsubst /usr/local/bin/
}

function configure_nginx() {
    cd /opt
    local ncpu_online=$( getconf _NPROCESSORS_ONLN )
    local ncpu_cpuset=
    local ncpu_quota=
    local ncpu_cpuset_v2=
    local ncpu_quota_v2=
    local cpuset=$( get_cgroup_v1_path "cpuset" ) && ncpu_cpuset=$( get_cpuset "$cpuset" "cpuset.effective_cpus" ) || ncpu_cpuset=$ncpu_online
    local cpu=$( get_cgroup_v1_path "cpu" ) && ncpu_quota=$( get_quota "$cpu" ) || ncpu_quota=$ncpu_online
    local cgroup_v2=$( get_cgroup_v2_path ) && ncpu_cpuset_v2=$( get_cpuset "$cgroup_v2" "cpuset.cpus.effective" ) || ncpu_cpuset_v2=$ncpu_online
    local cgroup_v2=$( get_cgroup_v2_path ) && ncpu_quota_v2=$( get_quota_v2 "$cgroup_v2" ) || ncpu_quota_v2=$ncpu_online
    local ncpu=$( printf "%s\n%s\n%s\n%s\n%s\n" \
                "$ncpu_online" \
                "$ncpu_cpuset" \
                "$ncpu_quota" \
                "$ncpu_cpuset_v2" \
                "$ncpu_quota_v2" \
                | sort -n \
                | head -n 1 )
    [ -f /etc/nginx/nginx.conf ] && mv /etc/nginx/nginx.conf /etc/nginx/nginx.conf.bak
    [ -f /etc/nginx/conf.d/default.conf ] && rm /etc/nginx/conf.d/default.conf
    cat > /etc/nginx/nginx.conf <<CONFIG
daemon on;
user  nginx;
worker_processes  auto;
load_module modules/ngx_http_modsecurity_module.so;
load_module modules/ngx_stream_geoip_module.so;
load_module modules/ngx_http_geoip_module.so;
load_module modules/ngx_http_image_filter_module.so;
load_module modules/ngx_http_js_module.so;
load_module modules/ngx_stream_js_module.so;
load_module modules/ngx_http_xslt_filter_module.so;
load_module modules/ngx_http_geoip2_module.so;
error_log  /var/log/nginx/error.log warn;
pid        /var/run/nginx.pid;
events {
    worker_connections  1024;
    multi_accept on;
}
http {
    include       mime.types;
    default_type  application/octet-stream;
    log_format  main  '\$remote_addr - \$remote_user [\$time_local] "\$request" '
                      '\$status \$body_bytes_sent "\$http_referer" '
                      '"\$http_user_agent" "\$http_x_forwarded_for"';
    access_log  /var/log/nginx/access.log  main;
    # don't limit traffic from private ips
    geo \$limit{
        default 1;
        172.0.0.0/8 0;
    }
    map \$limit \$limit_key {
        0 "";
        1 \$binary_remote_addr;
    }
    sendfile        off;
    #tcp_nopush     on;
    keepalive_timeout  65;
    gzip  on;
    gzip_types
      text/css
      text/javascript
      text/xml
      text/plain
      text/x-component
      application/javascript
      application/json
      application/xml
      application/rss+xml
      font/truetype
      font/opentype
      application/vnd.ms-fontobject
      image/svg+xml;
    gzip_static on;
    gzip_proxied        expired no-cache no-store private auth;
    gzip_disable        "MSIE [1-6]\.";
    gzip_vary           on;

    limit_conn_zone \$limit_key zone=addr:10m;
    limit_req_zone \$limit_key zone=perip:10m rate=1r/s;
    proxy_cache_path  /var/cache/nginx levels=1:2 keys_zone=STATIC:10m inactive=24h max_size=2g;
    proxy_cache_key \$request_method\$proxy_host\$request_uri;
    add_header X-Cache-Status \$upstream_cache_status;
    add_header X-Frame-Options SAMEORIGIN;
    add_header X-Content-Type-Options nosniff;
    add_header X-XSS-Protection "1; mode=block";
    add_header Strict-Transport-Security "max-age=63072000; includeSubDomains; preload";
    # link iso country codes from the geoip2 db
    geoip2 /etc/nginx/geoip/dbip-country-lite.mmdb {
        \$geoip2_data_country_code country iso_code;
    }
    # specify which locations to allow/block
    map \$geoip2_data_country_code \$allowed_country {
        default no;
        '' yes;
        US yes;
    }
    # map HSTS header
    map \$scheme \$hsts_header { https "max-age=31556926; includeSubDomains; preload"; }
    # map status to exclude from access log
    map \$status \$writelog { 444 0; default 1; }
    # map some user agent string to block
    map \$http_user_agent \$bad_client {
        default 0;
        ~*(360Spider) 1;
        ~*(aiHitBot|AhrefsBot) 1;
        ~*(betaBot|BlackWidow|Bolt|BLEXBot|BUbiNG) 1;
        ~*(CazoodleBot|CPython|CCBot|ChinaClaw|Curious|CRAZYWEBCRAWLER|Custo) 1;
        ~*(Default|DIIbot|DISCo|discobot) 1;
        ~*(Exabot|eCatch|ecxi|EirGrabber|EmailCollector|EmailSiphon|EmailWolf|ExtractorPro|EyeNetIE) 1;
        ~*(FlashGet|Findxbot) 1;
        ~*(GetRight|GetWeb!|Go!Zilla|Go-Ahead-Got-It|Go.*package.*|GrabNet|Grafula|GT::WWW|GuzzleHttp) 1;
        ~*(heritrix|HaosouSpider|HMView|HTTP::Lite|HTTrack) 1;
        ~*(ia_archiver|IDBot|id-search|id-search.org|InterGET|InternetSeer.com|IRLbot) 1;
        ~*(JetCar) 1;
        ~*(larbin|LeechFTP|Lightspeedsystems|litemage_walker|Link|LinksManager.com_bot|Lipperhey|linkwalker|LinkpadBot|lwp-trivial|ltx71) 1;
        ~*(Maxthon\$|Mail.RU_Bot|MegaIndex.ru|meanpathbot|MFC_Tear_Sample|microsoft.url|Microsoft-IIS|Mozilla.*Indy|Mozilla.*NEWT|MJ12bot|MSFrontPage) 1;
        ~*(Navroad|NearSite|NetAnts|NetLyzer.*FastProbe|NetSpider|NetZIP|Nutch) 1;
        ~*(Octopus) 1;
        ~*(PageGrabber|panscient.com|pavuk|PECL::HTTP|PeoplePal|pcBrowser|Pi-Monster|PHPCrawl|PleaseCrawl|psbot|prijsbest) 1;
        ~*(Qwantify) 1;
        ~*(RealDownload|ReGet|RedesScrapy|Rippers|RocketCrawler) 1;
        ~*(SBIder|Scrapy|Screaming|ScreenerBot|SEOprofiler|SeaMonkey\$|SeznamBot|SemrushBot|sitecheck.internetseer.com|SiteSnagger) 1;
        ~*(SmartDownload|Snoopy|SputnikBot|Steeler|SuperBot|SuperHTTP|Surfbot|sqlmap) 1;
        ~*(tAkeOut|Teleport|Toata|TwengaBot|Typhoeus) 1;
        ~*(URI::Fetch|User-Agent|UserAgent) 1;
        ~*(voltron|Vagabondo|VoidEYE|Visbot) 1;
        ~*(webalta|WebAuto|[Ww]eb[Bb]andit|WebCollage|WebCopier|WebFetch|WebLeacher|WebReaper|WebSauger|WebStripper|WebWhacker|WhatsApp) 1;
        ~*(WebZIP|Wget|Widow|Wotbox|WWW-Mechanize|WWWOFFLE) 1;
        ~*(zermelo|Zeus|Zeus.*Webster|ZyBorg) 1;
    }
    # custom error pages
    # error_page 403 /errors/403.html;
    # error_page 500 501 502 503 504 /errors/5xx.html;
    include /etc/nginx/conf.d/*.conf;
}
CONFIG
    sed -i.bak -r 's/^(worker_processes)(.*)$/#\1\2\n\1 '"$ncpu"';/' /etc/nginx/nginx.conf
    cat > /etc/nginx/options-ssl-nginx.conf <<CONFIG
ssl_session_cache shared:le_nginx_SSL:10m;
ssl_stapling on;
ssl_stapling_verify on;
ssl_session_timeout 5m;
ssl_session_tickets off;
ssl_protocols TLSv1.2 TLSv1.3;
ssl_prefer_server_ciphers on;
ssl_ciphers "TLS_CHACHA20_POLY1305_SHA256:TLS_AES_128_CCM_8_SHA256:TLS_AES_128_CCM_SHA2:ECDHE-RSA-AES256-GCM-SHA384:ECDHE-ECDSA-AES256-GCM-SHA384:DHE-DSS-AES256-GCM-SHA384:DHE-RSA-AES256-GCM-SHA384";
CONFIG
    cat > /etc/nginx/ssl-dhparams.pem <<CONFIG
